    },
    "Tracklist": {
      "type": "text"
    },
    "Song": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false
    },
    "artists": {
      "via": "records",
